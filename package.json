  "scripts": {
    "analyze": "next experimental-analyze",
    "build": "bun run --bun next build",
    "check": "bun run lint && bun run typecheck && bun run format:check",
    "db:check": "drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:seed": "bun run src/lib/db/seed.ts",
    "db:studio": "bun run drizzle-kit studio",
    "db:up": "bun run drizzle-kit up",
    "dev": "bun run --bun next dev --turbopack",
    "format:check": "oxfmt --check .",
    "format:write": "oxfmt --write .",
    "lint": "oxlint --type-aware .",
    "lint:fix": "oxlint . --fix",
    "preview": "bun run next build && bun run next start",
    "start": "bun run --bun next start",
    "typecheck": "bun run tsc --noEmit"
