{
  "name": "yopem",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "analyze": "bun run --bun next experimental-analyze",
    "build": "bun run --bun next build",
    "check": "bun run --bun lint && bun run --bun typecheck && bun run --bun format:check",
    "db:check": "bun run --bun drizzle-kit check",
    "db:drop": "bun run --bun drizzle-kit drop",
    "db:generate": "bun run --bun drizzle-kit generate",
    "db:migrate": "bun run --bun drizzle-kit migrate",
    "db:pull": "bun run --bun drizzle-kit introspect",
    "db:push": "bun run --bun drizzle-kit push",
    "db:studio": "bun run --bun drizzle-kit studio",
    "db:up": "bun run --bun drizzle-kit up",
    "dev": "bun run --bun next dev --turbopack",
    "format:check": "bun run --bun prettier --check \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:write": "bun run --bun prettier --write \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "bun run --bun eslint ./src",
    "lint:fix": "bun run --bun eslint ./src --fix",
    "preview": "bun run --bun next build && bun run --bun next start",
    "start": "bun run --bun next start",
    "typecheck": "bun run --bun tsc --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@next/third-parties": "16.1.3",
    "@openauthjs/openauth": "^0.4.3",
    "@orpc/client": "^1.13.1",
    "@orpc/server": "^1.13.1",
    "@orpc/tanstack-query": "^1.13.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-orm": "^0.45.0",
    "drizzle-zod": "^0.8.3",
    "js.foresight": "^3.3.3",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "next": "16.1.3",
    "next-international": "^1.3.1",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "server-only": "^0.0.1",
    "superjson": "^2.2.3",
    "tailwind-merge": "^3.4.0",
    "transliteration": "^2.3.5",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/bun": "^1.3.1",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.15.5",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@yopem/eslint-config": "^0.3.1",
    "@yopem/prettier-config": "^0.1.9",
    "@yopem/typescript-config": "^0.2.1",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.38.0",
    "lefthook": "^2.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "prettier": "@yopem/prettier-config/react",
  "packageManager": "bun@1.3.1",
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
