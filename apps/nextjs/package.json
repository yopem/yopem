{
  "name": "@yopem/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.33.1",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
    "@trpc/client": "^10.38.0",
    "@trpc/next": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@trpc/server": "^10.38.0",
    "@yopem/api": "^0.1.0",
    "@yopem/auth": "^0.1.0",
    "@yopem/db": "^0.1.0",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^18.19.80",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@yopem/eslint-config": "^0.2.0",
    "@yopem/prettier-config": "^0.1.0",
    "@yopem/tailwind-config": "^0.1.0",
    "@yopem/tsconfig": "^0.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "prettier": "^3.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@yopem/eslint-config/base",
      "@yopem/eslint-config/nextjs",
      "@yopem/eslint-config/react"
    ]
  },
  "prettier": "@yopem/prettier-config"
}
