{
  "name": "@repo/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/root.ts",
      "default": "./src/root.ts"
    },
    "./orpc": {
      "types": "./src/orpc.ts",
      "default": "./src/orpc.ts"
    },
    "./orpc/server": {
      "types": "./src/orpc/server.ts",
      "default": "./src/orpc/server.ts"
    },
    "./orpc/client": {
      "types": "./src/orpc/client.ts",
      "default": "./src/orpc/client.ts"
    },
    "./orpc/query": {
      "types": "./src/orpc/query.ts",
      "default": "./src/orpc/query.ts"
    },
    "./orpc/shared": {
      "types": "./src/orpc/shared.ts",
      "default": "./src/orpc/shared.ts"
    },
    "./schemas/api-keys": {
      "types": "./src/schemas/api-keys.ts",
      "default": "./src/schemas/api-keys.ts"
    }
  },
  "scripts": {
