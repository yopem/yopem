{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./src/schema/index.ts"
    },
    "./services/slug": {
      "types": "./src/services/slug.ts",
      "default": "./src/services/slug.ts"
    },
    "./services/tools": {
      "types": "./src/services/tools.ts",
      "default": "./src/services/tools.ts"
    },
    "./services/tags": {
      "types": "./src/services/tags.ts",
      "default": "./src/services/tags.ts"
    },
    "./services/categories": {
      "types": "./src/services/categories.ts",
      "default": "./src/services/categories.ts"
    },
    "./services/assets": {
      "types": "./src/services/assets.ts",
      "default": "./src/services/assets.ts"
    },
    "./services/user": {
      "types": "./src/services/user.ts",
      "default": "./src/services/user.ts"
    },
    "./services/admin": {
      "types": "./src/services/admin.ts",
      "default": "./src/services/admin.ts"
    }
  },
  "scripts": {
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "format:check": "oxfmt --check .",
    "format:write": "oxfmt --write .",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "db:generate": "bun run with-env drizzle-kit generate",
    "db:migrate": "bun run with-env drizzle-kit migrate",
    "db:studio": "bun run with-env drizzle-kit studio",
    "db:seed": "bun run with-env bun run src/seed.ts"
  },
  "dependencies": {
    "@repo/env": "workspace:*",
    "@repo/logger": "workspace:*",
    "@repo/shared": "workspace:*",
    "drizzle-orm": "catalog:drizzle",
    "drizzle-zod": "catalog:drizzle",
    "pg": "catalog:drizzle",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:tooling",
    "@types/pg": "catalog:drizzle",
    "@yopem/typescript-config": "catalog:tooling",
    "drizzle-kit": "catalog:drizzle",
    "eslint-plugin-oxfmt": "catalog:tooling",
    "oxfmt": "catalog:tooling",
    "oxlint": "catalog:tooling",
    "oxlint-tsgolint": "catalog:tooling",
    "typescript": "catalog:tooling"
  }
}
