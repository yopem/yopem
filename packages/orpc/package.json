{
  "name": "@repo/orpc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./shared": {
      "types": "./src/shared.ts",
      "default": "./src/shared.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./query": {
      "types": "./src/query.ts",
      "default": "./src/query.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "default": "./src/server.ts"
    }
  },
  "scripts": {
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "format:check": "oxfmt --check .",
    "format:write": "oxfmt --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@orpc/client": "catalog:orpc",
    "@orpc/server": "catalog:orpc",
    "@orpc/tanstack-query": "catalog:orpc",
    "@repo/logger": "workspace:*",
    "next": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:tooling",
    "@types/react": "catalog:",
    "@yopem/typescript-config": "catalog:tooling",
    "oxfmt": "catalog:tooling",
    "oxlint": "catalog:tooling",
    "oxlint-tsgolint": "catalog:tooling",
    "typescript": "catalog:tooling"
  }
}
